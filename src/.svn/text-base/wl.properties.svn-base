#分号切割,可以根据需求自行改变切割符号
game.split.symbol=;
#游戏日志正则表达式前缀
game.regular.filter.prefix=^(.+)#
#游戏文件名称的过滤
game.file.name.filter=wl.world2.formatlog;wl.world2.log;wl.statinfom;wl.world2.money

#武林(4)wl.world2.money过滤类型
wl.world2.money.filter.type=money_change
#1.
money_change.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+money_change : rid=([^:]*):l=([^:]*):mt=([^:]*):mc=([^:]*):r=([^:]*)

#武林(3)wl.statinfom过滤类型
wl.statinfom.filter.type=statinfom_1;statinfom_2;statinfom_3;statinfom_4;statinfom_5;statinfom_6
#1.
statinfom_1.has.index.number=true
statinfom_1.special.filter.key=STAT:Level
statinfom_1.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+STAT:\\bLevel\\b=(.+):(.+):(.+):(.+):([^:]+)$
#2.
statinfom_2.has.index.number=true
statinfom_2.special.filter.key=STAT:MoneyPut
statinfom_2.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+STAT:\\bMoneyPut\\b=(.+):(.+):(.+):(.+):([^:]+)$
#3.
statinfom_3.has.index.number=true
statinfom_3.special.filter.key=STAT:Money
statinfom_3.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+STAT:\\bMoney\\b=(.+):(.+):(.+):(.+):([^:]+)$
#4.
statinfom_4.has.index.number=true
statinfom_4.special.filter.key=STAT:ItemPut
statinfom_4.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+STAT:\\bItemPut\\b=(.+):(.+):(.+):(.+):([^:]+)$
#5.
statinfom_5.has.index.number=true
statinfom_5.special.filter.key=STAT:PutData
statinfom_5.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+STAT:\\bPutData\\b=(.+):(.+):(.+):(.+):([^:]+)$
#6.
statinfom_6.has.index.number=true
statinfom_6.special.filter.key=STAT:SysMail
statinfom_6.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+STAT:\\bSysMail\\b=(.+):(.+):(.+):(.+):([^:]+)$

#武林(2)wl.world2.log过滤类型
wl.world2.log.filter.type=itemtomoney;new_item_cash;playerstall;playerstalled;item_cash
#1.
item_cash.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+info\\s+:\\s+\\bitem_cash\\b,roleid=(\\d+),service_id=(\\d+),buy_item=(\\d+),cash_id=(\\d+),cash_count=(-?\\d+),order_count=(-?\\d+)$
#2.
itemtomoney.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+info\\s+:\\s+\\bitemtomoney\\b,roleid=(.+),count=(.+),item=(.+),money=(-?\\d+)$
#3.
new_item_cash.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+info\\s+:\\s+\\bnew_item_cash\\b,roleid=(\\d+),service_id=(\\d+),buy_item=(\\d+),buy_item_count=(\\d+),cash1_id=(\\d+),cash1_count=(\\d+),cash2_id=(\\d+),cash2_count=(\\d+),cash3_id=(\\d+),cash3_count=(\\d+),unbind_money=(\\d+),bind_money=(-?\\d+)$
#4.
playerstall.special.filter.key=playerstall,rolea
playerstall.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+info\\s+:\\s+\\bplayerstall\\b,rolea=(.+),roleb=(.+),count=(.+),item=(.+),price=([^:]+)$
#5.
playerstalled.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+info\\s+:\\s+\\bplayerstalled\\b,rolea=(.+),roleb=(.+),count=(.+),item=(.+),price=([^:]+)$


#武林(1)wl.world2.formatlog过滤类型
wl.world2.formatlog.filter.type=playerlogout;forbiduser_question;stockcommission;trade;accountlogin;rolelogout;deleterole;webtradesold;accountlogout;gmlog;forbiduser;task_complete;gamemailbox;regmailbox;gamemailbox_award;regmailbox_award;rolelogin;task_accept;task_abort;instance_player_enter;instance_player_award;shoptrade;addcash;stockbalance;clear360cash;clear360item

#1.
playerlogout.special.filter.key=playerlogout:userid
playerlogout.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+\\bplayerlogout\\b:userid=(\\d+):roleid=(\\d+):reborn_count=(\\d+):level=(\\d+):occupation=(\\d+):mapid=(\\d+):x=(-?\\d+):y=(-?\\d+):z=(-?\\d+):onlinetime=(-?\\d+)$
#2.
forbiduser_question.special.filter.key=formatlog:forbiduser:source
forbiduser_question.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:forbiduser:source=(.+):gmuserid=(.+):userid=(.+):zoneid=(.+):areaid=(.+):forbid_time=(.+):real_time=(.+):reason=(.+):cash_add=(.+):cash_used=(.+)
#3.
stockcommission.special.filter.key=formatlog:stockcommission:userid
stockcommission.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:\\bstockcommission\\b:userid=(.+):tid=(.+):price=(.+):volume=(.+)$
#4.
trade.special.filter.key=formatlog:trade:roleidA
trade.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:\\btrade\\b:roleidA=(.+):roleidB=(.+):moneyA=(.+):moneyB=(.+):objectsA=(.*):objectsB=(.*)$
#5.
accountlogin.special.filter.key=formatlog:login:account
accountlogin.must.contain.key=mid=
accountlogin.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:login:account=(.+):userid=(.+):sid=(.+):peer=(.+):mac=([^:]*):mid=([^:]*)$
#6.
rolelogout.special.filter.key=formatlog:rolelogout:userid
rolelogout.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:rolelogout:userid=(.+):roleid=(.+):localsid=(.+):time=(-?\\d+)$
#7.
deleterole.special.filter.key=formatlog:deleterole:roleid
deleterole.must.contain.key=create_time=
deleterole.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:deleterole:roleid=(.+):create_time=(.+)$
#8.
webtradesold.special.filter.key=formatlog:webtrade:webtradesold:role
webtradesold.must.contain.key=type=4
webtradesold.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:webtrade:webtradesold:role=(.+):sn=(.+):buyer_roleid=(.+):type=(4):itemid=(.+):item_count=(.+):money=(.+):role=(.+):petid=(.+):pet_count=(.+):level=(.+):reborn_cnt=(.+)$
#9.
accountlogout.special.filter.key=formatlog:logout:account
accountlogout.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:\\blogout\\b:account=(.+):userid=(.+):sid=(.+):peer=(.+):time=(-?\\d+):status=.+:mac=[^:]*:mid=([^:]*)$
#10.
gmlog.special.filter.key=formatlog:gmoperate:userid
gmlog.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:\\bgmoperate\\b:userid=(.+):type=(.+):content=(.*)$
#11.
forbiduser.special.filter.key=ACForbidUser
forbiduser.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:\\bACForbidUser\\b:GM=(.+):roleid=(.+):accountid=(.+):forbid_time=(.+):reason=(.+)
#12.
task_complete.special.filter.key=formatlog:task_complete:userid
task_complete.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:task_complete:userid=.+:roleid=(.+):taskid=(.+):lev=.+:levtype=.+:success=.+:gold=(.+):exp=(.+):sp=(.+):reputation=(.+):items=(.*)$
#13.
gamemailbox.special.filter.key=gamemailbox:send_roleid
gamemailbox.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+gamemailbox:send_roleid=(.+):receive_roleid=(.+):level=(.+):offline_time=(.+)$
#14.
regmailbox.special.filter.key=regmailbox:send_roleid
regmailbox.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+regmailbox:send_roleid=(.+):send_userid=(.+):receive_userid=(.+):receive_roleid=(.+):level=(.+):offline_seconds=(-?\\d+)$
#15.
gamemailbox_award.special.filter.key=gamemailbox_award:roleid
gamemailbox_award.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+gamemailbox_award:roleid=(.+):item=(.+):count=(.+):callback_roleid=(.+)$
#16.
regmailbox_award.special.filter.key=regmailbox_award:roleid
regmailbox_award.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+regmailbox_award:roleid=(.+):item=(.+):count=(.+)$
#17.
rolelogin.special.filter.key=formatlog:rolelogin:userid
rolelogin.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:rolelogin:userid=(.+):roleid=(.+):lineid=(.+):localsid=(.+):levtype=(-?\\d+)$
#18.
task_accept.special.filter.key=formatlog:task_accept:userid
task_accept.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:task_accept:userid=(.+):roleid=(.+):taskid=(.+):lev=(.+):levtype=(-?\\d+)$
#19.
task_abort.special.filter.key=formatlog:task_abort:userid
task_abort.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:task_abort:userid=(.+):roleid=(.+):taskid=(.+):lev=(.+):levtype=(-?\\d+)$
#20.
instance_player_enter.special.filter.key=formatlog:instance_player_enter:userid
instance_player_enter.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:instance_player_enter:userid=(.+):roleid=(.+):serial_id=(.+):gs_id=(.+):template_id=(.+):result=(.+):lev=(.+),levtype=([^:]+)$
#21.
instance_result.special.filter.key=formatlog:instance_result:serial_id
instance_result.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:instance_result:serial_id=(.+):gs_id=(.+):battle_id=(.+):world_tag=(.+):world_index=(.+):template_id=(.+):result=(.+):role_count=(.+)$
#22.
instance_player_award.special.filter.key=formatlog:instance_player_award:userid
instance_player_award.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:instance_player_award:userid=(.+):roleid=(.+):serial_id=(.+):gs_id=(.+):template_id=(.+):result=(.+):lev=(.+),levtype=([^:]+)$
#23.
shoptrade.special.filter.key=formatlog:gshop_trade:userid
shoptrade.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:\\bgshop_trade\\b:userid=(.+):roleid=(.+):order_id=(.+):item_id=(.+):expire=(.+):item_count=(.+):cash_need=(.+):cash_left=([^:]+):guid=.+$
#24.
addcash.special.filter.key=formatlog:addcash:userid
addcash.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:\\baddcash\\b:userid=(.+):oldserial=.+:newserial=.+:cash_add=(.+):delta=(.+)$
#25.
stockbalance.special.filter.key=formatlog:stockbalance:userid
stockbalance.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:\\bstockbalance\\b:userid=(.+):tid=(.+):volume=(.+):ack_volume=(.+):ack_money=(.+):cash=(.+):money=(.+):cash_buy=(.+):cash_sell=([^:]+)$
#26.
clear360cash.special.filter.key=formatlog:clear360
clear360cash.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:\\bclear360\\b:userid=(.+):logintime=(.+):cash_delta=(\\d+?);.*$
clear360cash.has.same.filter.key=clear360
#27.
clear360item.special.filter.key=formatlog:clear360
clear360item.regular.filter.key=(\\d+-\\d+-\\d+\\s+\\d+:\\d+:\\d+).+formatlog:\\bclear360\\b:userid=(.+):logintime=.+:cash_delta=\\d+?;(.+)$
clear360item.has.same.filter.key=clear360

